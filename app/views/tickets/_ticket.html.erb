<%= form_for ticket, remote: true do |f| %>
  <%= f.check_box :complete %>
  <%= f.submit "Update" %>
  <%= best_in_place ticket, :uw_name %>
  <%= best_in_place ticket, :loan_number %>
  <%= best_in_place ticket, :notes %>
  <%= best_in_place ticket, :second_signature, as: :checkbox, collection: {false: "No", true: "Yes"} %>
  <%= link_to "Delete", ticket,
                        data: {confirm: "Are you sure?", method: :delete},
                        remote: true %>
<% end %>
